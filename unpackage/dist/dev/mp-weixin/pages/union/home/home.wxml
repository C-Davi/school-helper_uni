<view><view class="nav"><block wx:for="{{nav}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="{{[(isActive===index?'nav-active':'')+' nav-list']}}" data-index="{{index}}" data-event-opts="{{[['tap',[['getActiveNav',['$event']]]]]}}" bindtap="__e">{{item}}</view></block></view><view class="main"><block wx:for="{{news}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="news" data-index="{{index}}" data-event-opts="{{[['tap',[['goDetailPage',['$event']]]]]}}" bindtap="__e"><image class="news-img" mode="aspectFill" src="{{logo}}"></image><text class="news-name">{{item.branch_name}}</text><block wx:if="{{apply}}"><view class="new-stu" data-id="{{item.id}}" data-event-opts="{{[['tap',[['toApply',['$event']]]]]}}" bindtap="__e"><image class="new-icon" src="../../../static/images/img/mt2.png"></image>纳新报名</view></block><text class="news-times">{{item.branch_info}}</text></view></block></view></view>