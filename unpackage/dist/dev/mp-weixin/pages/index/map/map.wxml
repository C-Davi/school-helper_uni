<view><view class="custom" style="{{('height: '+CustomBar+'px; padding-top: '+StatusBar+'px; background-color: var(--primary);')}}"><navigator class="custom-back" openType="navigateBack"></navigator><view class="custom-name">黄岛校区</view></view><swiper class="tab" current="{{current}}" nextMargin="645rpx"><block wx:for="{{tab}}" wx:for-item="item" wx:for-index="index" wx:key="index"><swiper-item><view class="{{['tab-list '+(tabNum===index?'active':'quiet')]}}" data-index="{{index}}" data-event-opts="{{[['tap',[['changeActive',['$event']]]]]}}" bindtap="__e">{{item.name}}</view></swiper-item></block></swiper><map style="{{('height: calc(100vh - 80vw - '+CustomBar+'px); width: 100vw;')}}" enable3D enableOverlooking enableTraffic showCompass showLocation id="map" includePoints="{{include}}" markers="{{markers}}" data-event-opts="{{[['markertap',[['getMarker',['$event']]]]]}}" bindmarkertap="__e"></map><scroll-view class="region" scroll-y scrollIntoView="{{view}}"><block wx:for="{{markers}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="region-list" data-id="{{item.id}}" idAttr="{{'region-'+item.id}}" data-event-opts="{{[['tap',[['getMarker',['$event']]]]]}}" bindtap="__e"><image class="region-list__pic" mode="aspectFit" src="{{logo}}"></image><text class="region-list__name">{{item.callout.content}}</text><button class="region-list__icon_share" open-type="share" title="分享青职定位" data-name="{{item.callout.content}}"><image class="region-list__share" src="../../../static/images/icon/share.svg"></image></button><image class="region-list__icon" data-id="{{item.id}}" src="../../../static/images/icon/nav.svg" data-event-opts="{{[['tap',[['openLoc',['$event']]]]]}}" bindtap="__e"></image></view></block></scroll-view></view>